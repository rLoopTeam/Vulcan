FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (134.112 0 0);

boundaryField
{
    defaultFaces
    {
        type            fixedValue;
        value           uniform (134.112 0 0);
    }
    inlet
    {

          type            waveTransmissive;
        field           U;
        phi             phi;
        rho             rho;
        psi             thermo:psi;
        gamma           1.4;
        fieldInf        (134.112 0 0);
        lInf            1.0;
        value           uniform (134.112 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    wall
    {
        type            slip;
    }
    floor
    {
        type            slip;
    }
    body
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    symmetry
    {
        type            slip;
    }
}

