FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 827.3709;

boundaryField
{
    defaultFaces
    {
        type            zeroGradient;
    }
    body
    {
        type            zeroGradient;
    }
    floor
    {
        type            slip;
    }
    inlet
    {
		type zeroGradient;
/*             type            waveTransmissive;*/
/*        field           p;*/
/*        phi             phi;*/
/*        rho             rho;*/
/*        psi             thermo:psi;*/
/*        gamma           1.4;*/
/*        fieldInf        827.3709;*/
/*        lInf            4.0;*/
/*        value           uniform 827.3709;*/
    }
    outlet
    {
 //       type            fixedValue;
//		value			uniform 827.3709;
   type            waveTransmissive;
        field           p;
        phi             phi;
        rho             rho;
        psi             thermo:psi;
        gamma           1.4;
        fieldInf        827.3709;
        lInf            1.0;
        value           uniform 827.3709;
    }
    wall
    {
        type            slip;
    }
}


