FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 2757.9;

boundaryField
{
    defaultFaces
    {
        type            zeroGradient;
    }
    body
    {
        type            zeroGradient;
    }
    floor
    {
        type            slip;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
	type           fixedValue;
       value           uniform 2757.9;    
    }
    wall
    {
        type            slip;
    }
}


