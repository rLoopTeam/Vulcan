FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 86132.6442721792;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
		value			$internalField;
    }
    wall
    {
        type            zeroGradient;
    }
    floor
    {
        type            zeroGradient;
    }
    body
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            zeroGradient;
    }
    symmetry
    {
        type            slip;
    }
}

